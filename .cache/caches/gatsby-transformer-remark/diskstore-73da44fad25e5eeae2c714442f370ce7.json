,"offset":801},"indent":[]}}],"position":{"start":{"line":7,"column":1,"offset":685},"end":{"line":7,"column":117,"offset":801},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"Az ok, amiért nem történik meg:","position":{"start":{"line":9,"column":1,"offset":804},"end":{"line":9,"column":32,"offset":835},"indent":[]}}],"position":{"start":{"line":9,"column":1,"offset":804},"end":{"line":9,"column":32,"offset":835},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"– titkolás","position":{"start":{"line":11,"column":1,"offset":838},"end":{"line":11,"column":11,"offset":848},"indent":[]}}],"position":{"start":{"line":11,"column":1,"offset":838},"end":{"line":11,"column":11,"offset":848},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"– konfliktuskerülés (életbevágó hiba)","position":{"start":{"line":13,"column":1,"offset":851},"end":{"line":13,"column":38,"offset":888},"indent":[]}}],"position":{"start":{"line":13,"column":1,"offset":851},"end":{"line":13,"column":38,"offset":888},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"– gyengeség (nem fejlődőképesség)","position":{"start":{"line":15,"column":1,"offset":891},"end":{"line":15,"column":34,"offset":924},"indent":[]}}],"position":{"start":{"line":15,"column":1,"offset":891},"end":{"line":15,"column":34,"offset":924},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"– önbizalomhiány","position":{"start":{"line":17,"column":1,"offset":927},"end":{"line":17,"column":17,"offset":943},"indent":[]}}],"position":{"start":{"line":17,"column":1,"offset":927},"end":{"line":17,"column":17,"offset":943},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"– lustaság","position":{"start":{"line":19,"column":1,"offset":946},"end":{"line":19,"column":11,"offset":956},"indent":[]}}],"position":{"start":{"line":19,"column":1,"offset":946},"end":{"line":19,"column":11,"offset":956},"indent":[]}},{"type":"paragr